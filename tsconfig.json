{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"target": "es5",
		"module": "es2015",
		"moduleResolution": "node",
		"lib": [
			"es2015",
			"dom"
		],
		"typeRoots": [
			"node_modules/@types"
		],
		"rootDir": "./src/",
		"outDir": "./public/",
	},
	"include": [
		"./src/"
	],
	"exclude": [
		"./src/html/"
	]
}